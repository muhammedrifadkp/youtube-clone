{
  "name": "youtube-clone",
  "version": "1.0.0",
  "description": "Complete YouTube clone with exact UI/UX and MongoDB backend",
  "main": "index.js",
  "scripts": {
    "test": "node test-youtube-clone.js",
    "setup": "cd backend && npm install && cd ../frontend && npm install",
    "dev": "concurrently \"cd backend && npm run dev\" \"cd frontend && npm start\"",
    "start:backend": "cd backend && npm run dev",
    "start:frontend": "cd frontend && npm start",
    "db:setup": "cd backend && npm run db:setup",
    "build": "cd frontend && npm run build",
    "install:all": "npm run setup"
  },
  "keywords": [
    "youtube",
    "clone",
    "react",
    "mongodb",
    "nodejs",
    "express",
    "material-ui",
    "video",
    "streaming"
  ],
  "author": "YouTube Clone Developer",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^7.6.0",
    "axios": "^1.6.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/youtube-clone.git"
  }
  
}
